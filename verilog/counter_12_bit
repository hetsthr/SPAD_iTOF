#! /usr/local/bin/vvp
:ivl_version "11.0 (devel)" "(s20150603-819-g462ee62)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/local/lib/ivl/system.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/local/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/local/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/local/lib/ivl/va_math.vpi";
S_0x130cea0 .scope module, "tb_counter_12_bit" "tb_counter_12_bit" 2 2;
 .timescale -9 -12;
v0x131d160_0 .var "clk", 0 0;
v0x131d230_0 .net "count", 11 0, v0x131cf30_0;  1 drivers
v0x131d300_0 .var "reset", 0 0;
S_0x130d030 .scope module, "c1" "counter_12_bit" 2 18, 3 1 0, S_0x130cea0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 12 "count";
v0x130d260_0 .net "clk", 0 0, v0x131d160_0;  1 drivers
v0x131cf30_0 .var "count", 11 0;
v0x131d010_0 .net "reset", 0 0, v0x131d300_0;  1 drivers
E_0x130cc70 .event posedge, v0x131d010_0, v0x130d260_0;
    .scope S_0x130d030;
T_0 ;
    %wait E_0x130cc70;
    %load/vec4 v0x131d010_0;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 12;
    %assign/vec4 v0x131cf30_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x131cf30_0;
    %addi 1, 0, 12;
    %assign/vec4 v0x131cf30_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x130cea0;
T_1 ;
    %delay 0, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x131d160_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x131d300_0, 0, 1;
    %vpi_call 2 7 "$dumpfile", "counter_test.vcd" {0 0 0};
    %vpi_call 2 8 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x130cea0 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x130cea0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x131d160_0;
    %inv;
    %store/vec4 v0x131d160_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x130cea0;
T_3 ;
    %delay 2000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x131d300_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x131d300_0, 0, 1;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_counter_12_bit.v";
    "counter_12_bit.v";
